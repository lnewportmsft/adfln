{
	"name": "dataflow1",
	"properties": {
		"sources": [
			{
				"dataset": {
					"referenceName": "MoviesDBDataset",
					"type": "DatasetReference"
				},
				"name": "Movies",
				"script": "source(output(movie as string,title as string,genres as string,year as integer,Rating as integer,{Rotton Tomato} as integer),allowSchemaDrift: false,validateSchema: false) ~> Movies"
			}
		],
		"sinks": [
			{
				"dataset": {
					"referenceName": "movie_dataflow_sink",
					"type": "DatasetReference"
				},
				"name": "sink1",
				"script": "DerivedColumn1 sink(saveMode:'overwrite' ) ~> sink1"
			}
		],
		"transformations": [
			{
				"name": "DerivedColumn1",
				"script": "Movies derive(cleantitle = ERROR_FUNCTION('')) ~> DerivedColumn1"
			}
		]
	}
}